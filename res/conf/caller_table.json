{
  "port": 33496,
  "start_ros_core": true,
  "start": ["mode_com"],
  "modes": [
    {
      "name": "agv",
      "description": "AGV小车启动",
      "executables": [
        {
          "name": "PTZControl",
          "path": "/data/cleaner_ws/install_nav/lib/control_holder",
          "working_dir": "/data/cleaner_ws/install_nav/lib/control_holder"
        },
        {
          "name": "controller_serial_node",
          "path": "/data/cleaner_ws/install_nav/lib/controller_serial",
          "working_dir": "/data/cleaner_ws/install_nav/lib/controller_serial"
        },
        {
          "name": "map_viewer",
          "path": "/data/cleaner_ws/install_nav/lib/global_path_planner",
          "working_dir": "/data/cleaner_ws/install_nav/lib/global_path_planner",
          "delay": 1000
        },
        {
          "name": "global_planner",
          "path": "/data/cleaner_ws/install_nav/lib/global_path_planner",
          "working_dir": "/data/cleaner_ws/install_nav/lib/global_path_planner",
          "delay": 1000
        },
        {
          "name": "rslidar_sdk_node_xyzir",
          "path": "/data/cleaner_ws/install_nav/lib/lidar_receiver",
          "working_dir": "/data/cleaner_ws/install_nav/lib/lidar_receiver",
          "params": [
            "middlemiddletop"
          ],
          "delay": 1000
        },
        {
          "name": "obstacle_detector",
          "path": "/data/cleaner_ws/install_nav/lib/obstacle_detector/",
          "working_dir": "/data/cleaner_ws/install_nav/lib/obstacle_detector/",
          "delay": 4000
        },
        {
          "name": "locate_3d_node",
          "path": "/data/cleaner_ws/install_nav/lib/locate_3d/",
          "working_dir": "/data/cleaner_ws/install_nav/lib/locate_3d/",
          "delay": 4000
        },
        {
          "name": "local_planner",
          "path": "/data/cleaner_ws/install_nav/lib/local_planner_node",
          "working_dir": "/data/cleaner_ws/install_nav/lib/local_planner_node",
          "delay": 1000
        }
      ]
    },
    {
      "name": "mode_com",
      "description": "公共模块",
      "executables": [
        {
          "name": "livox_ros_driver_node",
          "path": "/data/garbage_auto_ws/install/lib/livox_ros_driver/",
          "working_dir": "/data/garbage_auto_ws/install/lib/livox_ros_driver/"
        },
        {
          "name": "fault_monitor",
          "path": "/data/garbage_auto_ws/install/lib/fault_monitor/",
          "working_dir": "/data/garbage_auto_ws/install/lib/livox_ros_driver/"
        },
        {
          "name": "garbage_ui",
          "path": "/data/garbage_auto_ws/install/lib/garbage_ui",
          "working_dir": "/data/garbage_auto_ws/install/lib/garbage_ui"
        }
      ]
    },
    {
      "name": "crane_1",
      "description": "行车1的模块",
      "pre_exec": [
        "rosparam load /data/garbage_auto_ws/car_1/conf/octomap_bev.yaml",
        "rosparam load /data/garbage_auto_ws/car_1/conf/map_convert.yaml",
        "rosparam load /data/garbage_auto_ws/car_1/conf/grab_detector.yaml"
      ],
      "executables": [
        {
          "name": "octomap_bev_node",
          "path": "/data/garbage_auto_ws/car_1/install/lib/fault_monitor/",
          "params": ["1"]
        },
        {
          "name": "map_convert_node",
          "path": "/data/garbage_auto_ws/install/lib/laser_point_convert/",
          "params": ["1"]
        },
        {
          "name": "grab_detector_3d",
          "path": "/data/garbage_auto_ws/install/lib/grab_detector_3d/",
          "params": ["1"]
        }
      ]
    },
    {
      "name": "crane_2",
      "description": "行车2的模块",
      "pre_exec": [
        "rosparam load /data/garbage_auto_ws/car_2/conf/octomap_bev.yaml",
        "rosparam load /data/garbage_auto_ws/car_2/conf/map_convert.yaml",
        "rosparam load /data/garbage_auto_ws/car_2/conf/grab_detector.yaml"
      ],
      "executables": [
        {
          "name": "octomap_bev_node",
          "path": "/data/garbage_auto_ws/car_1/install/lib/fault_monitor/",
          "params": ["2"]
        },
        {
          "name": "map_convert_node",
          "path": "/data/garbage_auto_ws/install/lib/laser_point_convert/",
          "params": ["2"]
        },
        {
          "name": "grab_detector_3d",
          "path": "/data/garbage_auto_ws/install/lib/grab_detector_3d/",
          "params": ["2"]
        }
      ]
    }
  ]
}
